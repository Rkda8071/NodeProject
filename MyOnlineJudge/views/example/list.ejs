<!DOCTYPE html>
<html lang="en">
<%- include("../header"); %>
<body>
    <!-- Start your project here-->  
    <%- include("../menu"); %>
    <button 
        type="button" 
        class="btn btn-warning"
        onclick="location.href='/api/example/new'"
    >등록</button>
    <div class="row row-cols-2 row-cols-md-1">
    <!--Table-->
    <% index = 0 %>
    <table id="tablePreview" class="table" style="margin-left: 30px;">
    <!--Table head-->
      <thead>
        <tr>
          <th>#</th>
          <th>input Data</th>
          <th>output Data</th>
        </tr>
      </thead>
      <!--Table head-->
      <!--Table body-->
      <tbody>
        <% result.forEach(example => { %>
            <tr>
                <th scope="row"><%= ++index %></th>
                <td><%= example.input %></td>
                <td><%= example.output %></td>
            </tr>
        <% }) %>        
      </tbody>
      <!--Table body-->
    </table>
    <!--Table-->
    
    </div>
    <!-- End your project here-->

    <%- include("../footer"); %>

</body>
</html>