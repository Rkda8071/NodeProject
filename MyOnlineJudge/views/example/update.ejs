<!DOCTYPE html>
<html lang="en">
<%- include("../header"); %>
<body>
    <%- include("../menu"); %>
    <form id="updateForm">
        <!-- Start your project here-->  
        
        <!-- Material input -->
        <div class="md-form">
            <input 
                type="text"
                id="input"
                value="<%= result.input %>" 
                class="form-control"
                required
            />
            <label for="input">Input input_Data</label>
        </div>

        <div class="md-form">
            <input 
                type="text"
                id="output" 
                value="<%= result.output %>" 
                class="form-control"
                required
            />
            <label for="output">Input output_Data</label>
        </div>
        <button 
            type="submit" 
            class="btn btn-primary"
        >저장</button>
    </form>
    <!-- End your project here-->
    <%- include("../footer"); %>
    <script>
        $(function(){
            $("#updateForm").submit((event) => {
                event.preventDefault();
                //console.log("called");
                const title = $("#input").val();
                const text = $("#output").val();
                
                $.ajax({
                    url: "/api/example/<%=result._id %>",
                    type: "PUT",
                    data : {title, text},
                }).done(() => {
                    location.href = "/api/example/<%=result._id %>";
                }).fail((request, status, error) => {
                    alert(request.responseText);
                });
                //console.log(singer,title);
            });
        });
    </script>

</body>
</html>